<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jakub.kolonieparse" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".Start"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="Rezerwacja Koloni" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".OrganizerPanelActivty"
            android:label="@string/title_activity_organizer_panel_activty" >
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".SingleListItemOrg"
            android:label="@string/title_activity_single_list_item_org" >
        </activity>
        <activity
            android:name=".UserPanelActivity"
            android:label="@string/title_activity_user_panel" >
        </activity>
        <activity
            android:name=".SingleListItemUsr"
            android:label="@string/title_activity_single_list_item_usr" >
        </activity>
    </application>

</manifest>
